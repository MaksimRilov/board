{
  "root": true,
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "project": "tsconfig.json"
  },
  "extends": [ "airbnb-typescript/base", "plugin:@typescript-eslint/recommended", "plugin:prettier/recommended", "prettier", "prettier/@typescript-eslint"],
  "plugins": ["@typescript-eslint", "prettier", "import"],
  "rules": {
    "no-debugger": "off",
    "no-console": 0,
    "prettier/prettier": ["error", {
     "endOfLine":"auto",
     "printWidth": 100,
     "singleQuote": true
   }]
  }
}
